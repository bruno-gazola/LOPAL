                                </then>
                                <else>
                                    <if expression="escolha==&quot;ovo&quot;">
                                        <then>
                                            <output expression="&quot;R$ 19.00&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;R$ 24.00&quot;" newline="True"/>
                                        </else>
                                    </if>
