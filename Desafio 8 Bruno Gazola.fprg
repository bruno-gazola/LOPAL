        <body>
            <declare name="calabresa, costela, pernil, ovo, diet" type="String" array="False" size=""/>
            <declare name="escolha" type="String" array="False" size=""/>
            <output expression="&quot;As op&#231;&#245;es de lanche s&#227;o: Calabresa; Costela; Pernil; Ovo; Diet.&quot;" newline="True"/>
            <output expression="&quot;Qual ser&#225; sua escolha?&quot;" newline="True"/>
