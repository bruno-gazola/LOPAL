import java.util.Scanner;
public class Mainnn
{
    public static void main(String[] args) {
        Scanner leia = new Scanner(System.in);
        
        int n[] = new int[5];
        int i;
        
        for (i = 0; i < 5; i++){
            System.out.print("Digite o valor do Ã­ndice " + i + ": ");
            n[i] = leia.nextInt();
        }
        System.out.println("\n" + n[4] + ", " + n[3] + ", " + n[2] + ", " + n[1] + ", " + n[0]);
    }
}